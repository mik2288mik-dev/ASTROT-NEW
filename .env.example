# API Keys
# OpenAI API Key (optional, for future use)
OPENAI_API_KEY=your_openai_api_key_here

# Database URL (Replit Database API endpoint)
# Example: https://your-repl.repl.co or http://localhost:5000
# This should point to your backend API that connects to Replit Database
DATABASE_URL=

# Ephemeris Path (path to ephe folder with Swiss Ephemeris files)
# Example: ./ephe or /path/to/ephe
EPHE_PATH=

# Use SWE WASM (Swiss Ephemeris WebAssembly)
# Set to 'true' to enable WASM version of Swiss Ephemeris
USE_SWE_WASM=false
