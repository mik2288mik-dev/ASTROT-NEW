# API Keys
# OpenAI API Key (optional, for future use)
OPENAI_API_KEY=your_openai_api_key_here

# Database URL (Railway Database connection string)
# Example for PostgreSQL: postgresql://user:password@host:port/database
# Example for MySQL: mysql://user:password@host:port/database
# This should be your Railway Database connection string
DATABASE_URL=

# Ephemeris Path (path to ephe folder with Swiss Ephemeris files)
# Example: ./ephe or /path/to/ephe
EPHE_PATH=

# Use SWE WASM (Swiss Ephemeris WebAssembly)
# Set to 'true' to enable WASM version of Swiss Ephemeris
USE_SWE_WASM=false

# Next.js Public API URL (optional, for external API calls)
# Leave empty to use relative paths (/api)
NEXT_PUBLIC_API_URL=
